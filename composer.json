{
  "name": "stev/btipay",
  "description": "Librarie pentru integrarea sistemului de plati IPay al Bancii Transilvania",
  "type": "library",
  "authors": [
    {
      "name": "Stefan Matei",
      "email": "stev.matei@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Stev\\BTIPay\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "ext-json": "*",
    "ext-iconv": "*",
    "jms/serializer": "^3.29",
    "guzzlehttp/guzzle": "^6.5|^7.0",
    "alcohol/iso4217": "^4.1",
    "league/iso3166": "^4.1"
  },
  "config": {
    "platform": {
      "php": "8.0"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0|^8.0|^9.0",
    "doctrine/annotations": "^2.0"
  }
}
